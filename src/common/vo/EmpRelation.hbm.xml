<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.emp_relation.model.EmpRelationVO" table="emp_relation">
		<!-- <id name="emp_no,mgr_no" column="emp_no,mgr_no"/> -->
		<!-- <property name="dir_mgr" column="dir_mgr" /> -->

		<composite-id>
			<key-property name="emp_no" column="emp_no" />
			<key-property name="mgr_no" column="mgr_no" />
		</composite-id>
		<property name="dir_mgr" column="dir_mgr" />


	</class>
</hibernate-mapping>