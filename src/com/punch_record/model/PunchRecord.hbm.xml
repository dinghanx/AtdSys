<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.punch_record.model.PunchRecordVO" table="punch_record">
		<id name="pr_no" column="pr_no" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="pr_time" column="pr_time" />
		<property name="remarks" column="remarks" />

		<many-to-one name="empVO" class="com.employee.model.EmployeeVO"
			cascade="all" lazy="false">
			<column name="emp_no" />
		</many-to-one>
		<many-to-one name="ATVO" class="com.apply_type.model.ApplyTypeVO"
			cascade="all" lazy="false">
			<column name="at_no" />
		</many-to-one>

	</class>
</hibernate-mapping>